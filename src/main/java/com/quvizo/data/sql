CREATE TABLE "PRESENTATION_ASSETS"

(

"ID" BIGINT not null primary key
        GENERATED ALWAYS AS IDENTITY
        (START WITH 1, INCREMENT BY 1),   
"PID" NUMERIC,     
"ASSETID" NUMERIC,
"ADDED_ON" TIMESTAMP,
"BOOK" VARCHAR(255),
"CHAPTER" INTEGER,  
"FROMVERSE" INTEGER,
"TOVERSE" INTEGER   


);